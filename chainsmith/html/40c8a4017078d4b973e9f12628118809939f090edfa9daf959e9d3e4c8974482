	<option value='https://www.hexacorn.com/blog/2017/12/'> December 2017 </option>
	<option value='https://www.hexacorn.com/blog/2017/11/'> November 2017 </option>
	<option value='https://www.hexacorn.com/blog/2017/10/'> October 2017 </option>
	<option value='https://www.hexacorn.com/blog/2017/09/'> September 2017 </option>
	<option value='https://www.hexacorn.com/blog/2017/08/'> August 2017 </option>
	<option value='https://www.hexacorn.com/blog/2017/07/'> July 2017 </option>
	<option value='https://www.hexacorn.com/blog/2017/06/'> June 2017 </option>
	<option value='https://www.hexacorn.com/blog/2017/05/'> May 2017 </option>
	<option value='https://www.hexacorn.com/blog/2017/04/'> April 2017 </option>
	<option value='https://www.hexacorn.com/blog/2017/03/'> March 2017 </option>
