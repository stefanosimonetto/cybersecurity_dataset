<script type='text/javascript' src='https://www.webroot.com/blog/wp-content/plugins/custom-twitter-feeds/js/ctf-scripts.min.js?ver=1.8.4' id='ctf_scripts-js'></script>
<script>
window.jQuery && jQuery(function () {
	var doc = jQuery(document);
	var win = jQuery(window);
	var outerHeight = doc.outerHeight();
	var breakPoints = {
		20 : false,
		40 : false,
		60 : false,
		80 : false,
		100 : false
	};
	$(window).scroll(function () {
		if (window.SS) {
			var percent = (win.scrollTop() / (doc.height() - win.height())) * 100;
			if (percent >= 20 && !breakPoints[20]) {
				SS.EventTrack.rp("scroll-twenty"+window.location.pathname);
				breakPoints[20] = true;
			}
			if (percent >= 40 && !breakPoints[40]) {
				SS.EventTrack.rp("scroll-forty"+window.location.pathname);
				breakPoints[40] = true;
			}
			if (percent >= 60 && !breakPoints[60]) {
				SS.EventTrack.rp("scroll-sixty"+window.location.pathname);
				breakPoints[60] = true;
			}
			if (percent >= 80 && !breakPoints[80]) {
				SS.EventTrack.rp("scroll-eighty"+window.location.pathname);
				breakPoints[80] = true;
			}
			if (percent >= 98 && !breakPoints[100]) {
				SS.EventTrack.rp("scroll-one-hundred"+window.location.pathname);
				breakPoints[100] = true;
			}
		}
	});
});
</script>

