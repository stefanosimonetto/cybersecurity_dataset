}</p>
<p>function __php___shuffle_by_seed($ar, $srand_seed=null){<br />
if($srand_seed == null)<br />
$srand_seed = __php___make_seed($_SER&#8230;&#8230;&#8230;&#8230;&#8230;&#8230;.<br />
$ar_tmp = array();<br />
$i=0;<br />
while($i &lt; count($ar)) {<br />
srand($srand_seed);<br />
$r = rand(1,count($ar));<br />
if (isset($ar_tmp[$r-1]))<br />
++$srand_seed;<br />
else {<br />
$ar_tmp[$r-1] = $ar[$i];<br />
