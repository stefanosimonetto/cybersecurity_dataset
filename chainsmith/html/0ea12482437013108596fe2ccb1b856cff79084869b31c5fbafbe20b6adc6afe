    // C5 Theme Service (Header and Footer)
    (function($) {
        $(document).ready(function () {
            var baseURL = 'https://www.webroot.com/services/theme/webroot';
            if(
            	window.location.pathname.slice(5,9) == '/jp/'		//matches /jp/ (dev, origin)
