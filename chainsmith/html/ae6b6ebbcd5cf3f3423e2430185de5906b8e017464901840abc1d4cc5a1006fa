      <section class="hero">
      <div class="panel-pane pane-entity-field pane-node-field-hero"  >
  
      
  
  <div class="pane-content">
    
<div class="paragraphs-items paragraphs-items-field-hero paragraphs-items-field-hero-mini-hero paragraphs-items-mini-hero">
  
  <div class="field-hero">
    <div class="entity entity-paragraphs-item paragraphs-item-hero-image paragraphs-15631 view-mode-mini_hero on_node_blog">

  <div class="content">
    
